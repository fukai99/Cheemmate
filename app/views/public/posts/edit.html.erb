<%= form_with model: @post, url: post_path(@post), method: :patch do |f| %>

   <h4>コメント</h4>
  <%= f.text_area :content %>

	<h4>画像</h4>
  <%= f.file_field :images, multiple: true, accept: "image/*" %>

  <h4>Youtube url</h4>
  <%= f.text_field :youtube_url%>

  <h4>ジャンル</h4>
  <%#= f.collection_select :genre_id, Post.all, :id, :name, :prompt => "--選択してください--" %>

  <%= f.submit '変更を保存' %>
<% end %>
